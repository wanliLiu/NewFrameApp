<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CollapsingToolbarLayout">
        <attr name="collapseMode">
            <enum name="none" value="0"/>
            <enum name="pin" value="1"/>
            <enum name="parallax" value="2"/>
        </attr>
        <attr name="collapseParallaxMultiplier" format="float"/>
        <attr name="isForSpecailTitle" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="RingProgressBar">
        <attr name="ringColor" format="color" />
        <attr name="ringProgressColor" format="color" />
        <attr name="ringWidth" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="max" format="integer" />
        <attr name="textIsShow" format="boolean" />
        <attr name="progress" format="integer" />
        <attr name="ringPadding" format="dimension" />
        <attr name="style">
            <enum name="STROKE" value="0" />
            <enum name="FILL" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SvgImageView">
        <!--原色-->
        <attr name="svg_color" format="color" />
        <attr name="tint" format="color" />
        <attr name="android:tint" format="color" />
        <!--图片-->
        <attr name="android:src" />
        <attr name="srcCompat" />
    </declare-styleable>
</resources>